pipeline {
    agent any
 
    environment {
        AWS_DEFAULT_REGION = 'eu-central-1'
    }
 
    stages {
        stage('AWS ECR Describe Repositories') {
            steps {
                withCredentials([[$class: 'AmazonWebServicesCredentialsBinding', credentialsId: 'AKIAXMMDRHSK27H36ICN (AWS creds)']]) {
                    sh 'aws ecr describe-repositories --output json'
                }
            }
        }
        // Add more stages as needed
    }
}
